<table class="table">
  <tr>
    <th>名前</th>
    <th>フリガナ</th>
    <th>予約ステータス</th>
  </tr>
  <% @reservations.each do |reservation| %>
  <tr>
    <td>
      <%= link_to owner_reservation_path(reservation.id) do %>
        <%= reservation.end_user.last_name + " " + reservation.end_user.first_name %>
      <% end %>
      </td>
    <td><%= reservation.end_user.last_name_kana + " " + reservation.end_user.first_name_kana %></td>
    <td><%= reservation.status_i18n %></td>
  </tr>
  <% end %>
</table>
<div class="d-flex justify-content-center">
  <%= link_to "戻る", root_path, class:"btn btn-light" %>
</div>