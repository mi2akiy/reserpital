<table class="table">
  <tr>
    <th>名前</th>
    <th>フリガナ</th>
    <th>予約ステータス</th>
  </tr>
  <% @reservations.each do |reservation| %>
  <tr>
    <td>
      <%#= link_to owner_end_user_path(reservation.end_user.id) do %>
        <%= reservation.end_user.last_name + " " + reservation.end_user.first_name %>
      <%# end %>
      </td>
    <td><%= reservation.end_user.last_name_kana + " " + reservation.end_user.first_name_kana %></td>
    <td><%#= @end_user.reservations.where(hospital_id: @owner.hospital.id).order(created_at: :DESC).first.status_i18n %></td>
  </tr>
  <% end %>
</table>